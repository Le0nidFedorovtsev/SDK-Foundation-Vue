{
  "files.exclude": {
    "node_modules/": false,
    "package-lock.json": true,
    "composer.lock": true,
    "Dockerfile": true,
    ".github": false,
    ".git": true,
    ".gitignore": false,
    "captain-definition": true,
    "vite.config.js": false,
    "backend/vendor": false,
    "*/dist": true,
